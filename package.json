{
  "name": "@bryce-loskie/unocss-preset",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.9.0",
  "description": "Bryce Loskie's Unocss preset",
  "author": "@guygubaby",
  "license": "MIT",
  "homepage": "https://github.com/guygubaby/unocss-preset#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/unocss-preset.git"
  },
  "bugs": "https://github.com/guygubaby/unocss-preset/issues",
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-preset"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./mini": {
      "types": "./dist/mini.d.ts",
      "import": "./dist/mini.mjs",
      "require": "./dist/mini.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp -y --no-tag && pnpm publish --access public",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "up": "taze major -r -w -I"
  },
  "peerDependenciesMeta": {
    "unocss": {
      "optional": true
    }
  },
  "dependencies": {
    "@uni-helper/unocss-preset-uni": "^0.2.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@antfu/ni": "^24.3.0",
    "@types/node": "^22.13.5",
    "@unocss/autocomplete": "^66.1.0-beta.12",
    "@unocss/core": "^66.1.0-beta.12",
    "bumpp": "^10.1.0",
    "eslint": "^9.25.1",
    "esno": "^4.8.0",
    "pnpm": "^10.9.0",
    "rimraf": "^6.0.1",
    "taze": "^18.6.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "unocss": "^66.1.0-beta.12"
  }
}
