{
  "name": "@bryce-loskie/unocss-preset-mini",
  "type": "module",
  "version": "0.2.3",
  "packageManager": "pnpm@9.15.0",
  "description": "Bryce Loskie's Unocss preset",
  "author": "@guygubaby",
  "license": "MIT",
  "homepage": "https://github.com/guygubaby/unocss-preset-mini#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/unocss-preset-mini.git"
  },
  "bugs": "https://github.com/guygubaby/unocss-preset-mini/issues",
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-preset-mini"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish --access public",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "up": "taze major -r -w -I"
  },
  "peerDependenciesMeta": {
    "unocss": {
      "optional": true
    }
  },
  "dependencies": {
    "@uni-helper/unocss-preset-uni": "^0.2.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@antfu/ni": "^0.23.1",
    "@types/node": "^22.10.1",
    "@unocss/autocomplete": "^0.65.1",
    "@unocss/core": "^0.65.1",
    "bumpp": "^9.9.0",
    "eslint": "^9.16.0",
    "esno": "^4.8.0",
    "pnpm": "^9.15.0",
    "rimraf": "^6.0.1",
    "taze": "^0.18.0",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.65.1"
  }
}
